#!/bin/sh
xrandr --output DP-3.1 --primary --mode 2560x1440 --pos 4480x0 --rotate normal --output DP-3.2 --mode 2560x1440 --pos 7040x0 --rotate normal --output DP-0 --off --output DP-1 --off --output DP-2 --off --output DP-3 --off --output DP-4 --mode 1920x1080 --pos 0x0 --rotate normal --output DP-5 --mode 2560x1440 --pos 1920x0 --rotate normal --output DP-6 --off
